	<bean id="contactPersonService" class="core.commonapp.server.service.party.ContactPersonServiceImpl" />
	
	<!-- Payment -->
	<bean id="paymentService" class="core.commonapp.server.service.payment.PaymentServiceImpl"/>
	
	<!-- Product -->
	<bean id="productService" class="core.commonapp.server.service.product.ProductServiceImpl"/>
	
	<!-- SalesLead - ->
	<bean id="proposalService" class="core.commonapp.server.service.saleslead.ProposalServiceImpl"/>
	<bean id="salesLeadService" class="core.commonapp.server.service.saleslead.SalesLeadServiceImpl"/>
	-->
	<!-- Security -->
	<bean id="securityService" class="core.commonapp.server.service.security.SecurityServiceImpl"/>
	<bean id="securityGroupService" class="core.commonapp.server.service.security.SecurityGroupServiceImpl"/>
	<bean id="userService" class="core.commonapp.server.service.security.UserServiceImpl"/>
	
	<!-- Status -->
	<bean id="statusService" class="core.commonapp.server.service.status.StatusServiceImpl"/>
	
	<!-- Survey -->
	<bean id="surveyService" class="core.commonapp.server.service.survey.SurveyServiceImpl"/>
	
	<!-- Task -->
	<bean id="taskService" class="core.commonapp.server.service.task.TaskServiceImpl"/>
    
    
</beans>