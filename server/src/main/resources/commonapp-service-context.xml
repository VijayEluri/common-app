<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:lang="http://www.springframework.org/schema/lang"
    xmlns:util="http://www.springframework.org/schema/util"
    xsi:schemaLocation="
    http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/util 
    http://www.springframework.org/schema/util/spring-util-2.5.xsd
    http://www.springframework.org/schema/lang 
    http://www.springframework.org/schema/lang/spring-lang-2.5.xsd">
    
	<!-- Agreement -->
	<bean id="agreementService" class="core.commonapp.server.service.agreement.AgreementServiceImpl"/>
	
	<!-- Billing -->
	<bean id="billingAccountService" class="core.commonapp.server.service.billing.BillingAccountServiceImpl"/>
	
	<!-- ContactMech -->
	<bean id="contactMechService" class="core.commonapp.server.service.contact.ContactMechServiceImpl"/>
	<bean id="createContactMechService" class="core.commonapp.server.service.contact.CreateContactMechServiceImpl"/>
	<bean id="createPartyContactMechService" class="core.commonapp.server.service.contact.CreatePartyContactMechServiceImpl"/>
	<bean id="partyContactMechService" class="core.commonapp.server.service.contact.PartyContactMechServiceImpl"/>
	
	<!-- Geo -->
	<bean id="geoService" class="core.commonapp.server.service.geo.GeoServiceImpl"/>
	
	<!-- Party -->
	<bean id="organizationUnitService" class="core.commonapp.server.service.party.OrganizationUnitServiceImpl"/>
	<bean id="createPartyService" class="core.commonapp.server.service.party.CreatePartyServiceImpl"/>
	<bean id="partyService" class="core.commonapp.server.service.party.PartyServiceImpl"/>

